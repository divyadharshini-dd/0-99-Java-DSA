import java.util.Scanner;
public class odd {
    public static void main(String[] args) {
        Scanner in = new Scanner (System.in);
        int a = in.nextInt();
        if((a/2)*2==a)
        {
            System.out.println(a + " is even");
        }
        else
        {
            System.out.println(a + " is odd");
        }
    }
}
